datasource db {
  provider = "sqlite"
  url      = "file:../db/dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model users {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  username  String
  admin  String
  password  String
  role      String
}
